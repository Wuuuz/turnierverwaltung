{% extends '@App/base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}

    {% stylesheets 'bundles/app/css/base_left_menu/*' filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block sidecontent %}

<div id="wrapper">
    <div align="left" style="display:block;">
        <div id="uberschrift"style="max-height: 100px;">
            <div class="row">
                <a div class="uberschrift" href="#" button=""> </a>
                <a class= "col-lg-12" href="#" style="margin-left: 164px; padding-left:0px !important; height:60px;">
                    <h1 style="margin-top: 18px; margin-bottom: 18px; margin-left:5px; font-size:20px; color:rgba(150, 34, 34, 0.87);">{% block heading %}{% endblock %}</h1> </a>
            </div>
        </div>
    </div>

    <div class="navbar-default sidebar" role="navigation">
        <div class="sidebar-nav navbar-collapse">
            <ul class="nav" id="side-menu">
                {% block leftMenu %}{% endblock %}
            </ul>
        </div>
    </div>
</div>
    <div id="page-wrapper" style="min-height: 288px; ">
    {% block content %}{% endblock %}
    </div>
{% endblock %}